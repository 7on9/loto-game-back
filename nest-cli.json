{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/loto-game-back/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "assets/**/*",
        "watchAssets": true
      }
    ],
    "webpack": true,
    "tsConfigPath": "apps/loto-game-back/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/loto-game-back",
  "projects": {
    "loto-game-back": {
      "type": "application",
      "root": "apps/loto-game-back",
      "entryFile": "main",
      "sourceRoot": "apps/loto-game-back/src",
      "compilerOptions": {
        "tsConfigPath": "apps/loto-game-back/tsconfig.app.json"
      }
    }
  }
}
